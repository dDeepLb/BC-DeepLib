{
  "name": "bc-deeplib",
  "version": "1.0.1",
  "author": "dDeepLb",
  "description": "Package for easier development of BC mods without need to repeat myself in every mod",
  "license": "MIT",
  "files": [
    "/dist",
    ".types"
  ],
  "main": "./dist/DeepLib.js",
  "types": "./dist/DeepLib.d.ts",
  "scripts": {
    "build": "tsc && pnpm run copy",
    "watch": "tsc -w",
    "cleanbuild": "pnpm run clean && tsc && pnpm run copy",
    "lint": "eslint src/",
    "clean": "rimraf dist/",
    "copy": "copyfiles -u 1 src/**/*.html src/**/*.css dist/"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@stylistic/eslint-plugin": "^2.7.2",
    "bc-stubs": "^107.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.57.0",
    "globals": "^15.9.0",
    "rimraf": "^5.0.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.18.0"
  },
  "dependencies": {
    "bondage-club-mod-sdk": "^1.2.0"
  }
}
