{
  "name": "bc-deeplib",
  "version": "1.0.1",
  "author": "dDeepLb",
  "description": "Package for easier development of BC mods without need to repeat myself in every mod",
  "license": "MIT",
  "files": [
    "/dist",
    ".types"
  ],
  "main": "./dist/DeepLib.js",
  "types": "./dist/DeepLib.d.ts",
  "scripts": {
    "build": "tsc && pnpm run copy",
    "watch": "tsc -w",
    "cleanbuild": "pnpm run clean && tsc && pnpm run copy",
    "lint": "eslint src/",
    "clean": "rimraf dist/",
    "copy": "copyfiles -u 1 src/**/*.html src/**/*.css dist/"
  },
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "@stylistic/eslint-plugin": "^2.0.0",
    "bc-stubs": "^105.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.56.0",
    "globals": "^15.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0"
  },
  "dependencies": {
    "bondage-club-mod-sdk": "^1.2.0"
  }
}
